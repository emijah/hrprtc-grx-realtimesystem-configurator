<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.generalrobotix.ui.realtimesystem_configurator.PerspectiveFactory"
            id="com.generalrobotix.ui.realtimesystem_configurator.perspective"
            name="Realtime System Configurator"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.generalrobotix.ui.realtimesystem_configurator.category"
            name="Realtime System Configurator"/>
      <view
            category="Realtime System Configurator"
            class="com.generalrobotix.ui.realtimesystem_configurator.RTCListView"
            id="com.generalrobotix.ui.realtimesystem_configurator.rtclistview"
            name="RTC List"/>
      <view
            category="Realtime System Configurator"
            class="com.generalrobotix.ui.realtimesystem_configurator.TimingChartView"
            id="com.generalrobotix.ui.realtimesystem_configurator.timingchartview"
            name="Timing Chart"/>
      <view
            class="com.generalrobotix.ui.realtimesystem_configurator.RTSystemTopologyView"
            id="com.generalrobotix.ui.realtimesystem_configurator.topologyview"
            name="RTSysytem Topology"/>
      <view
            class="com.generalrobotix.ui.realtimesystem_configurator.BenchmarkOperatorView"
            id="com.generalrobotix.ui.realtimesystem_configurator.benchmarkoperator"
            name="Benchmark Operator"/>
      <view
            class="com.generalrobotix.ui.realtimesystem_configurator.ViewPart1"
            id="com.generalrobotix.ui.realtimesystem_configurator.view1"
            name="TestView"/>
   </extension> <extension
         point="org.eclipse.ui.editors">
      <editor
            default="false"
            id="jp.go.aist.rtm.systemeditor.editpartfactory"
            name="RTSystem Editor"/>
   </extension>
   
   

</plugin>
