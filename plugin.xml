<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.generalrobotix.ui.realtimesystem_configurator.PerspectiveFactory"
            icon="icons/grxrobotrt.png"
            id="com.generalrobotix.ui.realtimesystem_configurator.perspective"
            name="Realtime System Configurator"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.generalrobotix.ui.realtimesystem_configurator.category"
            name="Realtime System Configurator"/>
      <view
            category="Realtime System Configurator"
            class="com.generalrobotix.ui.realtimesystem_configurator.RTCListView"
            icon="icons/grxrobotrt.png"
            id="com.generalrobotix.ui.realtimesystem_configurator.rtclistview"
            name="RTC List"/>
      <view
            category="Realtime System Configurator"
            class="com.generalrobotix.ui.realtimesystem_configurator.TimingChartView"
            icon="icons/grxrobotrt.png"
            id="com.generalrobotix.ui.realtimesystem_configurator.timingchartview"
            name="Timing Chart"/>
      <view
            class="com.generalrobotix.ui.realtimesystem_configurator.RTSystemTopologyView"
            icon="icons/grxrobotrt.png"
            id="com.generalrobotix.ui.realtimesystem_configurator.topologyview"
            name="RTSysytem Topology"/>
      <view
            class="com.generalrobotix.ui.realtimesystem_configurator.BenchmarkOperatorView"
            icon="icons/grxrobotrt.png"
            id="com.generalrobotix.ui.realtimesystem_configurator.benchmarkoperator"
            name="Benchmark Operator"/>
      <view
            class="com.generalrobotix.ui.realtimesystem_configurator.BenchmarkResultExplorer"
            icon="icons/grxrobotrt.png"
            id="com.generalrobotix.ui.realtimesystem_configurator.benchmarkresult_explorer"
            name="Benchmark Result Explorer"/>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="com.generalrobotix.ui.realtimesystem_configurator.actionset"
            label="com.generalrobotix.ui.realtimesystem_configurator.actionset">
         <action
               class="com.generalrobotix.ui.realtimesystem_configurator.actionset.TopologyViewZoonInAction"
               icon="icons/zoomin.png"
               id="com.generalrobotix.ui.realtimesystem_configurator.topologyviewzoomin"
               label="com.generalrobotix.ui.realtimesystem_configurator.topologyviewzoomout"
               style="toggle"/>
      </actionSet>
   </extension>
   
   

</plugin>
